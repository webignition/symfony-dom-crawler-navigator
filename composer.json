{
    "name": "webignition/symfony-panther-dom-crawler-navigator",
    "description": "Navigate around a symfony panther dom crawler using convenience methods",
    "keywords": [],
    "homepage": "https://github.com/webignition/symfony-panther-dom-crawler-navigatorr",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "webignition@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\SymfonyDomCrawlerNavigator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\SymfonyDomCrawlerNavigator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": "./vendor/bin/phpcs src tests --colors --standard=PSR12",
        "static-analysis": "./vendor/bin/phpstan analyse src tests --level=7",
        "test": "./vendor/bin/phpunit --colors=always",
        "ci": [
            "@composer cs",
            "@composer static-analysis",
            "@composer test"
        ]
    },
    "require": {
        "php": ">=7.4",
        "symfony/panther": "^0.7",
        "webignition/web-driver-element-collection": ">=0.8,<1",
        "webignition/php-dom-element-locator": ">=0.4,<1",
        "webignition/php-dom-element-identifier": ">=0.14,<1",
        "php-webdriver/webdriver": "^1.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.75",
        "phpstan/phpstan-mockery": "^0.12.12",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5",
        "webignition/base-panther-test-case": ">=0.9,<1"
    },
    "config": {
        "sort-packages": true
    }
}
